{
  "name": "@shikitor/core",
  "version": "0.0.1",
  "description": "Shikitor core",
  "scripts": {
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NWYLZW/shikitor.git",
    "directory": "packages/core",
    "homepage": "https://github.com/NWYLZW/shikitor/tree/master/packages/core"
  },
